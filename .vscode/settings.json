{
  "python.defaultInterpreterPath": "./.venv/Scripts/python.exe",
  "python.terminal.activateEnvironment": true,
  "python.analysis.extraPaths": [
    "./apps"
  ],
  "files.associations": {
    "*.py": "python"
  },
  "files.encoding": "utf8",
  "files.eol": "\n",
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "terminal.integrated.cwd": "${workspaceFolder}",
  "python.envFile": "${workspaceFolder}/.env",
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.pyd": true,
    "**/.Python": true,
    "**/build": true,
    "**/develop-eggs": true,
    "**/dist": false,
    "**/downloads": true,
    "**/eggs": true,
    "**/.eggs": true,
    "**/lib": true,
    "**/lib64": true,
    "**/parts": true,
    "**/sdist": true,
    "**/var": true,
    "**/*.egg-info": true,
    "**/.pytest_cache": true,
    "**/.coverage": true,
    "**/htmlcov": true,
    "**/.tox": true,
    "**/.cache": true,
    "**/.mypy_cache": true,
    "**/.DS_Store": true
  }
}